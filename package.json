  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "react-animate-on-scroll": "^2.1.5",
    "react-top-loading-bar": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.5"
  }
